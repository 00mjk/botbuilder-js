<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>MultipleFlowAppLogic</CodeBehindClass>
      <CodeBehindNamespace>Microsoft.Bots.Rade.Data.MultipleFlow</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <Entity name="a" src="PhraseTopic"></Entity>
      <Entity name="b" src="PhraseTopic"></Entity>
      <DialogFlow name="Flow 1" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Module 1" from="Prompt"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
          <Transition to="Feedback" from="Module 1"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>in flow 1. type value of a</Feedback>
          </FeedbackOneOf>
          <FeedbackOneOf forTurn="1">
            <Feedback>reprompt flow 1. type value of a</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_a"></CodeRecognizer>
          <Example includeForLU="false" useAsIntentTip="false"></Example>
          <EntityRef name="a" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback">
          <Feedback>flow 1. value of a is {a}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
        <DialogFlowRef name="Module 1" refTo="Flow 2"></DialogFlowRef>
      </DialogFlow>
      <DialogFlow name="Flow 2" isRootFlow="false">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Feedback" from="Prompt"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>in flow 2. type value of b</Feedback>
          </FeedbackOneOf>
          <FeedbackOneOf forTurn="1">
            <Feedback>reprompt flow 2. type value of b</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_b"></CodeRecognizer>
          <EntityRef name="b" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback">
          <Feedback>flow 2. value of b is {b}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
    <PhraseTopic name="a" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="b" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
