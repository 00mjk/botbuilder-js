    <Task name="task1">
      <LUISRecognizer intentID="1234-5678" intentName="Greet" appID="e71640d2-1e57-47d6-8daf-e8c5eb258902" key="8bf384673e2e4ae5b514d95a9370bd07"></LUISRecognizer>
      <Entity name="Name" src="LUIS" entityType="Name" appID="e71640d2-1e57-47d6-8daf-e8c5eb258902"></Entity>
