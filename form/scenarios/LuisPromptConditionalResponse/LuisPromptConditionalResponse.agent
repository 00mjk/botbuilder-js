<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass></CodeBehindClass>
      <CodeBehindNamespace></CodeBehindNamespace>
    </About>
    <Task name="task">
      <LUISRecognizer intentID="1234-5678" intentName="bookTable" appID="fabef6b3-3a7f-42c0-a421-b047e2466460" key="8bf384673e2e4ae5b514d95a9370bd07"></LUISRecognizer>
      <Entity name="partySize" src="LUIS" entityType="simple" appID="fabef6b3-3a7f-42c0-a421-b047e2466460"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Feedback" from="Prompt"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>How can I help you?</Feedback>
          </FeedbackOneOf>
          <LUISRecognizer intentID="1234-5678" intentName="bookTable" appID="fabef6b3-3a7f-42c0-a421-b047e2466460" key="8bf384673e2e4ae5b514d95a9370bd07"></LUISRecognizer>
          <EntityRef name="partySize" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback">
          <Feedback>[Conditional1]</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
    <ConditionalResponseTemplate name="Conditional1" onRun="func">
      <SwitchCase>
        <Case value="small">
          <Feedback>{partySize} needs a small table</Feedback>
        </Case>
        <Case value="medium">
          <Feedback>{partySize} needs a medium table</Feedback>
        </Case>
        <Case value="large">
          <Feedback>{partySize} needs a large table</Feedback>
        </Case>
      </SwitchCase>
    </ConditionalResponseTemplate>
  </AgentDefinition>
</Agent>
