<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>MultipleFeedbackPromptAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <Entity name="a" src="PhraseTopic"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Feedback" from="Prompt"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>feedback 1</Feedback>
            <Feedback>feedback 2</Feedback>
            <Feedback>feedback 3</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_a"></CodeRecognizer>
          <EntityRef name="a" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback">
          <Feedback>thanks {a}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
    <PhraseTopic name="a" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
