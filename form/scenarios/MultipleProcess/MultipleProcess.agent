<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>MultipleProcessAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole.Agent</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Process 1">
          <Transition to="Process 1" from="Start"></Transition>
          <Transition to="Process 2" from="Process 1"></Transition>
          <Transition to="Process 3" from="Process 2"></Transition>
          <Transition to="Return" from="Process 3"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Return name="Return" outcome="Success"></Return>
        <Process name="Process 1" onRun="Process1_onRun"></Process>
        <Process name="Process 2" onRun="Process2_onRun"></Process>
        <Process name="Process 3" onRun="Process3_onRun"></Process>
      </DialogFlow>
    </Task>
  </AgentDefinition>
</Agent>
