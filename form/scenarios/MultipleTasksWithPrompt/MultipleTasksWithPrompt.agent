<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>MultipleTasksWithPromptAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole.Agent</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="recognize_1"></CodeRecognizer>
      <Entity name="a" src="PhraseTopic"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Feedback" from="Prompt"></Transition>
          <Transition to="Process 1" from="Feedback"></Transition>
          <Transition to="Return" from="Process 1"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>this is task 1 enter a value for a</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_a"></CodeRecognizer>
          <EntityRef name="a" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback">
          <Feedback>a has value {a}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
        <Process name="Process 1" onRun="a2x"></Process>
      </DialogFlow>
    </Task>
    <Task name="task 2">
      <CodeRecognizer onRun="recognize_2"></CodeRecognizer>
      <Entity name="b" src="PhraseTopic"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Feedback" from="Prompt"></Transition>
          <Transition to="Process 1" from="Feedback"></Transition>
          <Transition to="Return" from="Process 1"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>this is task 2 enter a value for b</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_b"></CodeRecognizer>
          <EntityRef name="b" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback">
          <Feedback>b has value {b}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
        <Process name="Process 1" onRun="b2y"></Process>
      </DialogFlow>
    </Task>
    <Task name="task 3">
      <CodeRecognizer onRun="recognize_3"></CodeRecognizer>
      <Entity name="x" src="Context"></Entity>
      <Entity name="y" src="Context"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="ContextEntities2entitiesProcess">
          <Transition to="ContextEntities2entitiesProcess" from="Start"></Transition>
          <Transition to="Feedback" from="ContextEntities2entitiesProcess"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Process name="ContextEntities2entitiesProcess" onRun="contextEntities2entities"></Process>
        <Respond name="Feedback">
          <Feedback>you said {x} {y}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
    <PhraseTopic name="a" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="b" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
