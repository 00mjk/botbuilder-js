<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>MultipleProcessMultiplePromptsSingleFeedbackNoCodeAppLogic</CodeBehindClass>
      <CodeBehindNamespace>MultipleProcessMultiplePromptsSingleFeedbackNoCode</CodeBehindNamespace>
    </About>
    <Task name="MultipleProcessMultiplePromptsSingleFeedbackNoCode">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <Entity name="entity1" src="PhraseTopic"></Entity>
      <Entity name="entity2" src="PhraseTopic"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Process 1">
          <Transition to="Process 1" from="Start"></Transition>
          <Transition to="Prompt 1" from="Process 1"></Transition>
          <Transition to="Process 2" from="Prompt 1"></Transition>
          <Transition to="Prompt 2" from="Process 2"></Transition>
          <Transition to="Feedback 1" from="Prompt 2"></Transition>
          <Transition to="Return" from="Feedback 1"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Return name="Return" outcome="Success"></Return>
        <Process name="Process 1" onRun="funcProcess1"></Process>
        <Prompt name="Prompt 1">
          <FeedbackOneOf forTurn="0">
            <Feedback>This is a prompt for entity1</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity1"></CodeRecognizer>
          <EntityRef name="entity1" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Process name="Process 2" onRun="funcProcess2"></Process>
        <Prompt name="Prompt 2">
          <FeedbackOneOf forTurn="0">
            <Feedback>This is a prompt for entity2</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity2"></CodeRecognizer>
          <EntityRef name="entity2" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback 1">
          <Feedback>This is the response with no code behind. Entities captured are entity1 = {entity1} and entity2 = {entity2}</Feedback>
        </Respond>
      </DialogFlow>
    </Task>
    <PhraseTopic name="entity1" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="entity2" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
