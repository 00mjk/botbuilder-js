<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>WideBranchingAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole.Agent</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <Entity name="entity" src="PhraseTopic"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Decision 1" from="Prompt"></Transition>
          <Transition to="Return" from="Feedback 1"></Transition>
          <Transition to="Return" from="Feedback 2"></Transition>
          <Transition to="Return" from="Feedback 3"></Transition>
          <Transition to="Return" from="Feedback 4"></Transition>
          <Transition to="Feedback 1" from="Decision 1" whenValueIs="a"></Transition>
          <Transition to="Feedback 2" from="Decision 1" whenValueIs="b"></Transition>
          <Transition to="Feedback 3" from="Decision 1" whenValueIs="c"></Transition>
          <Transition to="Feedback 4" from="Decision 1" whenValueIs="d"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for entity</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity"></CodeRecognizer>
          <EntityRef name="entity" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback 1">
          <Feedback>Feedback 1 {entity}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
        <Respond name="Feedback 2">
          <Feedback>Feedback 2 {entity}</Feedback>
        </Respond>
        <Respond name="Feedback 3">
          <Feedback>Feedback 3 {entity}</Feedback>
        </Respond>
        <Respond name="Feedback 4">
          <Feedback>Feedback 4 {entity}</Feedback>
        </Respond>
        <Process name="Decision 1" onRun="decision"></Process>
      </DialogFlow>
    </Task>
    <PhraseTopic name="entity" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
