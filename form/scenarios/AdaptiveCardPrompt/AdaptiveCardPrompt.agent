<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass></CodeBehindClass>
      <CodeBehindNamespace></CodeBehindNamespace>
    </About>
    <Task name="task">
      <CodeRecognizer onRun="greeting_func"></CodeRecognizer>
      <Entity name="entity_partySize" src="code"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Feedback" from="Prompt"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback adaptiveCardSrc="card space.json">How can I help you?</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="partySize_func"></CodeRecognizer>
          <EntityRef name="entity_partySize" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Respond name="Feedback">
          <Feedback>Your party size is {entity_partySize}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
  </AgentDefinition>
</Agent>
