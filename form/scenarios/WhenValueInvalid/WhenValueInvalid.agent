<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>BranchingAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole.Agent</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <Entity name="a" src="PhraseTopic"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Decision 1">
          <Transition to="Feedback 1" from="Decision 1" whenValueIs="yes"></Transition>
          <Transition to="Feedback 1" from="Decision 1" whenValueIs="no"></Transition>
          <Transition to="Return" from="Feedback 1"></Transition>
        </Transitions>
        <Process name="Decision 1" onRun="func"></Process>
        <Respond name="Feedback 1">
          <Feedback>Hello</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
    <PhraseTopic name="a" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
