<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>MultiplePromptAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole.Agent</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <Entity name="a" src="PhraseTopic" refTo="a"></Entity>
      <Entity name="b" src="PhraseTopic" refTo="b"></Entity>
      <Entity name="c" src="PhraseTopic" refTo="c"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt">
          <Transition to="Prompt" from="Start"></Transition>
          <Transition to="Prompt 2" from="Prompt"></Transition>
          <Transition to="Prompt 3" from="Prompt 2"></Transition>
          <Transition to="Return" from="Prompt 3"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for a</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_a"></CodeRecognizer>
          <EntityRef name="a" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Return name="Return" outcome="Success"></Return>
        <Prompt name="Prompt 2">
          <FeedbackOneOf forTurn="0">
            <Feedback>you said {a} now give me a value for b</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_b"></CodeRecognizer>
          <EntityRef name="b" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Prompt name="Prompt 3">
          <FeedbackOneOf forTurn="0">
            <Feedback>you have {a} and {b} now enter a value for c</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_c"></CodeRecognizer>
          <EntityRef name="c" promptIfMissing="true"></EntityRef>
        </Prompt>
      </DialogFlow>
    </Task>
    <PhraseTopic name="a" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="b" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="c" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
