<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>DeepBranchingAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole.Agent</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <CodeRecognizer onRun="hi"></CodeRecognizer>
      <Entity name="entity1" src="PhraseList"></Entity>
      <Entity name="entity2" src="PhraseList"></Entity>
      <Entity name="entity3" src="PhraseList"></Entity>
      <Entity name="entity4" src="PhraseList"></Entity>
      <Entity name="entity5" src="PhraseList"></Entity>
      <Entity name="entity6" src="PhraseList"></Entity>
      <Entity name="entity7" src="PhraseList"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Prompt 1">
          <Transition to="Prompt 1" from="Start"></Transition>
          <Transition to="Decision 1" from="Prompt 1"></Transition>
          <Transition to="Prompt 2" from="Decision 1" whenValueIs="left"></Transition>
          <Transition to="Prompt 3" from="Decision 1" whenValueIs="right"></Transition>
          <Transition to="Prompt 4" from="Decision 2" whenValueIs="left"></Transition>
          <Transition to="Prompt 5" from="Decision 2" whenValueIs="right"></Transition>
          <Transition to="Prompt 6" from="Decision 3" whenValueIs="left"></Transition>
          <Transition to="Prompt 7" from="Decision 3" whenValueIs="right"></Transition>
          <Transition to="Feedback 1" from="Decision 4" whenValueIs="left"></Transition>
          <Transition to="Feedback 2" from="Decision 4" whenValueIs="right"></Transition>
          <Transition to="Decision 2" from="Prompt 2"></Transition>
          <Transition to="Decision 3" from="Prompt 3"></Transition>
          <Transition to="Decision 4" from="Prompt 4"></Transition>
          <Transition to="Decision 5" from="Prompt 5"></Transition>
          <Transition to="Decision 6" from="Prompt 6"></Transition>
          <Transition to="Decision 7" from="Prompt 7"></Transition>
          <Transition to="Feedback 3" from="Decision 5" whenValueIs="left"></Transition>
          <Transition to="Feedback 4" from="Decision 5" whenValueIs="right"></Transition>
          <Transition to="Return" from="Feedback 1"></Transition>
          <Transition to="Feedback 5" from="Decision 6" whenValueIs="left"></Transition>
          <Transition to="Feedback 6" from="Decision 6" whenValueIs="right"></Transition>
          <Transition to="Feedback 7" from="Decision 7" whenValueIs="left"></Transition>
          <Transition to="Feedback 8" from="Decision 7" whenValueIs="right"></Transition>
          <Transition to="Return" from="Feedback 2"></Transition>
          <Transition to="Return" from="Feedback 3"></Transition>
          <Transition to="Return" from="Feedback 4"></Transition>
          <Transition to="Return" from="Feedback 5"></Transition>
          <Transition to="Return" from="Feedback 6"></Transition>
          <Transition to="Return" from="Feedback 7"></Transition>
          <Transition to="Return" from="Feedback 8"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Prompt name="Prompt 1">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter value for entity1</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_entity1"></CodeRecognizer>
          <EntityRef name="entity1" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Return name="Return" outcome="Success"></Return>
        <Process name="Decision 1" onRun="decision1"></Process>
        <Process name="Decision 2" onRun="decision2"></Process>
        <Process name="Decision 3" onRun="decision3"></Process>
        <Process name="Decision 4" onRun="decision4"></Process>
        <Prompt name="Prompt 2">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for entity2</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_entity2"></CodeRecognizer>
          <EntityRef name="entity2" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Prompt name="Prompt 3">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for entity3</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_entity3"></CodeRecognizer>
          <EntityRef name="entity3" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Prompt name="Prompt 4">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for entity4</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_entity4"></CodeRecognizer>
          <EntityRef name="entity4" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Prompt name="Prompt 5">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for entity5</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_entity5"></CodeRecognizer>
          <EntityRef name="entity5" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Prompt name="Prompt 6">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for entity6</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_entity6"></CodeRecognizer>
          <EntityRef name="entity6" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Prompt name="Prompt 7">
          <FeedbackOneOf forTurn="0">
            <Feedback>enter a value for entity7</Feedback>
          </FeedbackOneOf>
          <CodeRecognizer onRun="entity_entity7"></CodeRecognizer>
          <EntityRef name="entity7" promptIfMissing="true"></EntityRef>
        </Prompt>
        <Process name="Decision 5" onRun="decision5"></Process>
        <Respond name="Feedback 1">
          <FeedbackPerModality>
            <Speak>feedback1</Speak>
            <Display>feedback1 {entity1} {entity2} {entity4}</Display>
          </FeedbackPerModality>
        </Respond>
        <Process name="Decision 6" onRun="decision6"></Process>
        <Process name="Decision 7" onRun="decision7"></Process>
        <Respond name="Feedback 2">
          <FeedbackPerModality>
            <Speak>feedback2</Speak>
            <Display>feedback2 {entity1} {entity2} {entity4}</Display>
          </FeedbackPerModality>
        </Respond>
        <Respond name="Feedback 3">
          <FeedbackPerModality>
            <Speak>feedback3</Speak>
            <Display>feedback3 {entity1} {entity2} {entity5}</Display>
          </FeedbackPerModality>
        </Respond>
        <Respond name="Feedback 4">
          <FeedbackPerModality>
            <Speak>feedback4</Speak>
            <Display>feedback4 {entity1} {entity2} {entity5}</Display>
          </FeedbackPerModality>
        </Respond>
        <Respond name="Feedback 5">
          <FeedbackPerModality>
            <Speak>feedback5</Speak>
            <Display>feedback5 {entity1} {entity3} {entity6}</Display>
          </FeedbackPerModality>
        </Respond>
        <Respond name="Feedback 6">
          <FeedbackPerModality>
            <Speak>feedback6</Speak>
            <Display>feedback6 {entity1} {entity3} {entity6}</Display>
          </FeedbackPerModality>
        </Respond>
        <Respond name="Feedback 7">
          <FeedbackPerModality>
            <Speak>feedback7</Speak>
            <Display>feedback7 {entity1} {entity3} {entity7}</Display>
          </FeedbackPerModality>
        </Respond>
        <Respond name="Feedback 8">
          <FeedbackPerModality>
            <Speak>feedback8</Speak>
            <Display>feedback8 {entity1} {entity3} {entity7}</Display>
          </FeedbackPerModality>
        </Respond>
      </DialogFlow>
    </Task>
    <PhraseTopic name="entity1" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="entity2" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="entity3" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="entity4" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="entity5" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="entity6" scenario="Dictation"></PhraseTopic>
    <PhraseTopic name="entity7" scenario="Dictation"></PhraseTopic>
  </AgentDefinition>
</Agent>
