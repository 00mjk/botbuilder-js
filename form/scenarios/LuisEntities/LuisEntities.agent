<Agent xmlns="http://schemas.microsoft.com/ConversationalAgent/1.0">
  <AgentDefinition lang="en-us">
    <About>
      <CodeBehindClass>LuisIntentFeedbackAppLogic</CodeBehindClass>
      <CodeBehindNamespace>AgentConsole.Agent</CodeBehindNamespace>
    </About>
    <Task name="task1">
      <LUISRecognizer intentID="1234-5678" intentName="bookTable" appID="0009c6c3-c6b0-4e4d-b87e-4c082314afe9"></LUISRecognizer>
      <Entity name="partySize" src="LUIS" entityType="simple" appID="0009c6c3-c6b0-4e4d-b87e-4c082314afe9"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Feedback">
          <Transition to="Feedback" from="Start"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Respond name="Feedback">
          <Feedback>Your party size is {partySize}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
    <Task name="task2">
      <LUISRecognizer intentID="1234-5678" intentName="orderFood" appID="0009c6c3-c6b0-4e4d-b87e-4c082314afe9"></LUISRecognizer>
      <Entity name="foodType" src="LUIS" entityType="simple" appID="0009c6c3-c6b0-4e4d-b87e-4c082314afe9"></Entity>
      <Entity name="foodSize" src="LUIS" entityType="simple" appID="0009c6c3-c6b0-4e4d-b87e-4c082314afe9"></Entity>
      <Entity name="pickupOrDelivery" src="LUIS" entityType="simple" appID="0009c6c3-c6b0-4e4d-b87e-4c082314afe9"></Entity>
      <DialogFlow name="Dialog Flow" isRootFlow="true">
        <Transitions initialState="Feedback">
          <Transition to="Feedback" from="Start"></Transition>
          <Transition to="Return" from="Feedback"></Transition>
        </Transitions>
        <Initial name="Start"></Initial>
        <Respond name="Feedback">
          <Feedback>Food type is {foodType} and the size {foodSize} and this order is for {pickupOrDelivery}</Feedback>
        </Respond>
        <Return name="Return" outcome="Success"></Return>
      </DialogFlow>
    </Task>
  </AgentDefinition>
</Agent>
